{
    "name": "codesinging/laravel-douyin",
    "description": "抖音开放平台SDK",
    "license": "MIT",
    "authors": [
        {
            "name": "codesinging",
            "email": "codesinging@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "laravel/framework": "^8.80"
    },
    "autoload": {
        "psr-4": {
            "CodeSinging\\LaravelDouyin\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodeSinging\\LaravelDouyin\\DouyinServiceProvider"
            ],
            "aliases": {
                "douyin": "CodeSinging\\LaravelDouyin\\DouyinFacade"
            }
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    }
}
